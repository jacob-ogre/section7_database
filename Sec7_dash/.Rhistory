sidebarMenu(
menuItem("Inputs", icon = icon("bar-chart-o"), tabName = "tabOne"
),
menuItem("Data Selection",
icon=icon("sliders"),
selectInput(
inputId="year",
label="Year",
choices=c("All",1,2,3),
selected="All"
),
selectInput(
inputId="letter",
label="Letter",
choices=c("All", "A", "B"),
selected="All"
)
)
)
)
body <- dashboardBody(
tabItems(
tabItem("tabOne",
box(title = "Box One",
width = 12,
height = "100px",
status = "success",
solidHeader = TRUE,
collapsible = TRUE,
#                     verbatimTextOutput("boxOneText")
verbatimTextOutput("cur_year")
),
box(title = "Box Two",
width = 12,
height = "100px",
status = "success",
solidHeader = TRUE,
collapsible = TRUE,
verbatimTextOutput("cur_lett")
)
)
)
)
shinyApp(
ui = dashboardPage(header, sidebar, body),
server = function(input, output) {
output$boxOneText <- renderText({
paste("A test designed to provoke an emotional response")
})
output$cur_year <- renderText({
input$year
})
output$cur_lett <- renderText({
input$letter
})
}
)
